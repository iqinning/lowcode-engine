    "@alilc/lowcode-plugin-designer": "1.0.0-beta.17",
    "@alilc/lowcode-plugin-outline-pane": "1.0.0-beta.17",
    "@alilc/lowcode-shell": "1.0.0-beta.17",
    "@alilc/lowcode-utils": "1.0.0-beta.17",
